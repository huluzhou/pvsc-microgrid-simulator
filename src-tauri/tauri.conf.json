{
  "productName": "光储充微电网模拟器",
  "version": "0.1.0",
  "identifier": "com.pvsc.microgrid-simulator",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run ensure-kernel-dir",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [{"title": "光储充微电网模拟器", "width": 1280, "height": 800, "minWidth": 1280, "minHeight": 600, "resizable": true, "fullscreen": false}],
    "security": {"csp": null}
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": false,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {"webviewInstallMode": {"type": "skip"}},
    "resources": {
      "../dist/python-kernel/": "python-kernel"
    }
  },
  "plugins": {"shell": {"open": true}}
}
